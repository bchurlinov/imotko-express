ENV=development

# Database
DATABASE_URL="postgresql://USER:PASSWORD@localhost:5432/imotkodb"
DIRECT_URL="postgresql://USER:PASSWORD@localhost:5432/imotkodb"

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# Facebook OAuth / Pixel
FACEBOOK_CLIENT_ID=your-facebook-client-id
FACEBOOK_CLIENT_SECRET=your-facebook-client-secret
NEXT_PUBLIC_FACEBOOK_PIXEL_ID=your-facebook-pixel-id

# Third-party integrations
GOOGLE_PLACES_API_KEY=your-google-places-key
AWS_SES_KEY=your-aws-ses-access-key
AWS_SES_SECRET_KEY=your-aws-ses-secret-key
AWS_SES_REGION=us-east-1
IMOTKO_EMAIL=contact@imotko.mk
OPENAI_API_KEY=sk-your-openai-key
CRON_SECRET=replace-with-random-secret

# Supabase (shared across RN + Express)
NEXT_PUBLIC_SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key
SUPABASE_JWT_SECRET=your-supabase-jwt-secret
SUPABASE_JWKS_URL=https://your-project-ref.supabase.co/auth/v1/.well-known/jwks.json

# Cache Configuration
CACHE_ENABLED=true                              # Enable/disable caching (default: true, auto-disabled in test env)
CACHE_MAX_SIZE=500                              # Maximum number of cache entries (default: 500)
CACHE_TTL_DEFAULT=300000                        # Default TTL in milliseconds - 5 minutes (default: 300000)
CACHE_TTL_GET_PROPERTIES_SERVICE=1800000        # Properties service cache TTL - 30 minutes (default: 1800000)
CACHE_TTL_GET_AGENCY_BY_REFERER=300000          # Agency by referer cache TTL - 5 minutes (default: 300000)
CACHE_LOGGING=true                              # Enable cache operation logging (default: true in development)
